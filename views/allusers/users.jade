extends ../layout

block content
  .page-header
    h3 All Users 
    
  ul
    for user in users
    
        .row
          .col-sm-4.user-body
              a(href='/allusers/users/user', style='color: #fff')          
                img(src="#{user.profile.picture}", class='profile-pic', width='100', height='100')
              p.profile-name= user.profile.name
              a(href="#{user.profile.website}").profile-website Website
              p.profile-name= user.profile.age
      
  
